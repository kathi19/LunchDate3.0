<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="10dp"
        android:text="Profil"
        android:textSize="35dp" />





    <EditText
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="24dp"
        android:ems="25"
        android:inputType="textPersonName"
        android:text="Name"
        android:layout_marginLeft="160dp"
        android:layout_marginRight="24dp" />

    <EditText
        android:id="@+id/alter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="24dp"
        android:ems="25"
        android:inputType="textPersonName"
        android:text="Alter"
        android:layout_marginLeft="160dp"
        android:layout_marginRight="24dp" />

    <EditText
        android:id="@+id/studiengang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="24dp"
        android:ems="25"
        android:inputType="textPersonName"
        android:text="Studiengang"
        android:layout_marginLeft="160dp"
        android:layout_marginRight="24dp" />

    <EditText
        android:id="@+id/beginn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="24dp"
        android:ems="25"
        android:inputType="textPersonName"
        android:text="Studienbeginn"
        android:layout_marginLeft="160dp"
        android:layout_marginRight="24dp" />

    <TextView
        android:id="@+id/hobby"
        android:layout_width="129dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="295dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Interessen"
        android:textSize="25dp" />

    <CheckBox
        android:id="@+id/sport"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="330dp"
        android:text="Sport"
        android:textSize="20dp" />

    <CheckBox
        android:id="@+id/lesen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Lesen"
        android:textSize="20dp" />

    <CheckBox
        android:id="@+id/reisen"
        android:layout_width="107dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="230dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Reisen"
        android:textSize="20dp" />

    <CheckBox
        android:id="@+id/Freunde"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="395dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Freunde"
        android:textSize="20dp" />

    <CheckBox
    android:id="@+id/spielen"
    android:layout_width="117dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="120dp"
    android:layout_marginLeft="120dp"
    android:layout_marginTop="395dp"
    android:layout_marginEnd="24dp"
    android:layout_marginRight="24dp"
    android:text="Zocken"
    android:textSize="20dp" />

    <CheckBox
        android:id="@+id/Feiern"
        android:layout_width="117dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="230dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="395dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Feiern"
        android:textSize="20dp" />

    <Button
        android:id="@+id/aktualisieren"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="105dp"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Aktualisieren"
        android:textSize="18dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="138dp"
        android:layout_height="180dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="100dp"

        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        app:srcCompat="@drawable/profile" />


</RelativeLayout>